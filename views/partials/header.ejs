        <div class="container px-5 border-bottom">
            <a class="navbar-brand mb-0 h1" href="/">Alibazon</a>
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link fw-bold text-primary" href="/mens">Mens</a>
                </li>
                <li class="nav-item me-5">
                    <a class="nav-link fw-bold text-primary" href="/womens">Womens</a>
                </li>
                <% if (!token) { %>
                <li class="nav-item">
                    <a class="nav-link" href="/signup">Sign Up</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/signin">Sign In</a>
                </li>
                <% } %>
                <% if (token) { %>
                <li class="nav-item">
                    <a class="nav-link" href="/logout">Logout</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/cart">Cart</a>
                </li>
                <% } %>
            </ul>
        </div>
    </nav>
</div>